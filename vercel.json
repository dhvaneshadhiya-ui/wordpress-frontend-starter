{
  "buildCommand": "node scripts/fetch-wp-content.js && vite build && node scripts/build-static.js && node scripts/generate-seo-files.js",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "npm install",
  "functions": {
    "api/wp/[...path].js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "rewrites": [
    { "source": "/api/wp/:path*", "destination": "/api/wp/[...path]" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
